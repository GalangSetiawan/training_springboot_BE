	private boolean valDurasiDiskonValid(String idMi010, String idMi001, CustomerProdukTarifDetail detailTarif) {
		
		EAdminDiskon eAdminDiskon = adminDiskonService.getMaxDisTerakhirBdskNourutMaster(idMi010, idMi001, detailTarif.jnstrf, detailTarif.nourut);
		
		if (eAdminDiskon != null) {
			
			int diskeDetailTarif = detailTarif.jumdur;
			if (detailTarif.satdur.equals("TAHUN")) {
				diskeDetailTarif = diskeDetailTarif * 12;
			}
			
			if (diskeDetailTarif < eAdminDiskon.getDiske()) {
				batchError("customerProdukTarif.komplit.durasi.diskon.overlap.admin");
				return false;
			}
		}
		return true;
	}
		
	private boolean valPeriodeDiskonValid(String idMi010, String idMi001, CustomerProdukTarifDetail detailTarif) {
		
		EAdminDiskon eAdminDiskon = adminDiskonService.getMaxDisTerakhirBdskNourutMaster(idMi010, idMi001, detailTarif.jnstrf, detailTarif.nourut);
		
		if (eAdminDiskon != null) {
			
			if (detailTarif.tglstop != null && !detailTarif.tglstop.equals("")) {
				int tahunBulanDetailTarif = Integer.valueOf(TimeUtil.convertDateToYyyyMmDd(detailTarif.tglstop).substring(0, 6));
				int tahunBulanAdmin       = Integer.valueOf(eAdminDiskon.getTahun() + eAdminDiskon.getBulan());
				
				if (tahunBulanDetailTarif < tahunBulanAdmin) {
					batchError("customerProdukTarif.komplit.periode.diskon.overlap.admin");
					return false;
				}				
			}
		}
		return true;
	}
		
