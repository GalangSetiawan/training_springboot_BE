		editDetails(newETransaksiJurnalHeader, pojo);
		editDetailMappings(newETransaksiJurnalHeader, pojo);
		editDetailGrupAkumulasi(newETransaksiJurnalHeader, pojo);
		editDetailMappingsExclude(newETransaksiJurnalHeader, pojo);

		if (isErrorDetail) {
