package co.id.sofcograha.base.exceptions;

public class CustomSerializationException extends RuntimeException {
	
	/**
	 * 
	 */
	private static final long serialVersionUID = 7693950596064575372L;

	public CustomSerializationException(String message) {
		super(message);
	}
	
	public CustomSerializationException(String message, Throwable cause) {
		super(message, cause);
	}
}
