package co.id.sofcograha.base.exceptions;

public class CustomSearchParameterException extends RuntimeException {
	
	/**
	 * 
	 */
	private static final long serialVersionUID = 1782310086003682217L;
	public Object hint;

	public CustomSearchParameterException(String message, Object... hint) {
		super(message);
		this.hint = hint;
	}

	public CustomSearchParameterException(String message, Throwable cause, Object... hint) {
		super(message, cause);
		this.hint = hint;
	}
}
