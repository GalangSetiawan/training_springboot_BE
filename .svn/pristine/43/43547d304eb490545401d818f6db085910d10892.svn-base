	public void prosesBatalInvoiceOtomatisTunggal(String tahun, String bulan, String idMi010, String idMi001) {
		
		valHarusProsesTerakhir(tahun, bulan);
		throwBatchError();
		
		prosesUtamaBatal(idMi010, idMi001, tahun, bulan);
		
	}
